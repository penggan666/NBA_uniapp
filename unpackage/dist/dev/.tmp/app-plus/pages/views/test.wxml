<view class="content"><view class="input-group"><view class="input-row border"><text class="title">账号：</text><input class="m-input" type="text" clearable focus placeholder="请输入账号" data-event-opts="{{[['input',[['__set_model',['','account','$event',[]]]]]]}}" value="{{account}}" bindinput="__e"/></view><view class="input-row"><text class="title">密码：</text><input type="password" displayable placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view></view><view class="btn-row"><button class="primary" type="primary" data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" bindtap="__e">登录</button></view><view class="action-row"><navigator url="../views/reg">注册账号</navigator></view><block wx:if="{{hasProvider}}"><view class="oauth-row" style="{{'top:'+(positionTop+'px')+';'}}"><block wx:for="{{providerList}}" wx:for-item="provider" wx:for-index="__i0__" wx:key="value"><view class="oauth-image"><image src="{{provider.image}}" data-event-opts="{{[['tap',[['oauth',['$0'],[[['providerList','value',provider.value,'value']]]]]]]}}" bindtap="__e"></image></view></block></view></block></view>